{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": {
    "0": {
      "type": "package",
      "package": {
        "name": "drupal/phing-drush-task",
        "version": "1.2",
        "source": {
          "url": "https://git.drupal.org/project/phingdrushtask.git",
          "type": "git",
          "reference": "7.x-1.2"
        },
        "autoload": {
          "classmap": [
            "DrushTask.php"
          ]
        },
        "include-path": [
          "."
        ]
      }
    },
    "1": {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.6.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.2/chosen_v1.6.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    "2": {
      "type": "package",
      "package": {
        "name": "enyo/dropzone",
        "version": "5.1.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/enyo/dropzone/archive/v5.1.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    "3": {
      "type": "package",
      "package": {
        "name": "desandro/imagesloaded",
        "version": "3.2.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/desandro/imagesloaded/archive/v3.2.0.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    "4": {
      "type": "package",
      "package": {
        "name": "desandro/masonry",
        "version": "3.3.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/desandro/masonry/archive/v3.3.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "composer/installers": "^1.0.20",
    "drupal-composer/drupal-scaffold": "~1",
    "cweagans/composer-patches": "~1.0",
    "drupal/core": "8.4.5",
    "drush/drush": "~8.1",
    "drupal/config_installer": "1.*",
    "behat/behat": "^3.1.0",
    "drupal/migrate_source_csv": "2.0.0",
    "drupal/migrate_plus": "4.x-dev",
    "drupal/migrate_tools": "4.*",
    "drupal/config_devel": "1.*",
    "drupal/drupal-extension": "^3.1.5",
    "drupal/paragraphs": "1.*",
    "drupal/address": "1.*",
    "drupal/ui_patterns": "1.*",
    "drupal/field_group": "1.*",
    "drupal/inline_entity_form": "1.0.0-beta1",
    "drupal/honeypot": "1.24.0",
    "drupal/captcha": "1.0.0-beta1",
    "drupal/recaptcha": "^2.3",
    "drupal/adminimal_theme": "1.3.0",
    "drupal/twig_field_value": "1.1.0",
    "drupal/migrate_google_sheets": "1.x-dev",
    "drupal/eu_cookie_compliance": "1.0-beta7",
    "drupal/pathauto": "^1.0",
    "drupal/token": "~1",
    "drupal/ctools": "3.*",
    "drupal/metatag": "^1.5",
    "drupal/xmlsitemap": "1.0.0-alpha2",
    "drupal/admin_toolbar": "1.*",
    "drupal/adminimal_admin_toolbar": "1.*",
    "drupal/link_attributes": "1.*",
    "drupal/redirect": "^1.1",
    "drupal/maxlength": "1.0.0-beta1",
    "drupal/file_entity": "2.x-dev",
    "drupal/dropzonejs": "1.*",
    "drupal/devel": "^1.0@RC",
    "drupal/image_widget_crop": "^2.0",
    "drupal/entity_browser": "^1.0",
    "drupal/chosen": "^2.3",
    "harvesthq/chosen": "^1.6",
    "enyo/dropzone": "^5.1.1",
    "desandro/imagesloaded": "^3.2.0",
    "desandro/masonry": "^3.3.2",
    "drupal/geofield": "1.0-alpha4",
    "drupal/geocoder": "2.*",
    "drupal/empty_page": "^1.1",
    "drupal/menu_block": "^1.4",
    "drupal/imce": "^1.6",
    "drupal/entityqueue": "^1.0@alpha",
    "drupal/linkit": "^4.3",
    "drupal/console": "^1.4",
    "drupal/webform": "^5.0@RC",
    "drupal/sitemap": "^1.3",
    "drupal/redis": "^1.0@RC",
    "drupal/filemime": "^1.0",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/rename_admin_paths": "^1.1",
    "drupal/login_security": "^1.3",
    "drupal/backup_migrate": "4.x-dev",
    "drupal/smtp": "1.x-dev",
    "drupal/extlink": "^1.0",
    "drupal/rules": "3.x-dev",
    "drupal/broken_link": "^3.0",
    "drupal/seckit": "^1.0@alpha",
    "drupal/block_titlelink": "1.x-dev",
    "drupal/block_class": "1.x-dev",
    "abraham/twitteroauth": "^0.7.4"
    "drupal/share_everywhere": "^1.1"
  },
  "require-dev": {
    "phing/phing": "2.12.*",
    "drupal/phing-drush-task": "^1.2",
    "drupal/coder": "8.2.*",
    "behat/mink": "~1.6",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "^1.1@dev",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "2.*",
    "drupal/migrate_devel": "1.x-dev"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "bin/phpcs --config-set installed_paths vendor/drupal/coder/coder_sniffer"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ]
    },
    "patches-file": "composer.patches.json"
  }
}
